                  <Label layoutX="56.0" layoutY="140.0" prefHeight="17.0" prefWidth="50.0" text="Project:" />
                  <Label layoutX="316.0" layoutY="140.0" prefHeight="17.0" prefWidth="102.0" text="Life Cycle Step:" />
